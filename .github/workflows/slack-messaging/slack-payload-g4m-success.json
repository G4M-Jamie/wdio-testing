{
    "blocks": [
        {
            "type": "section",
            "text": {
                "type": "plain_text",
                "text": ":gear: Desktop Gear4music test suite has Passed :tada:  PASSED ${{ env.PASSED }} .   FAILED ${{ env.FAILED }}",
                "emoji": true
            }
        },
        {
            "type": "section",
            "text": {
                "type": "plain_text",
                "text": ":large_green_circle: Tests Passed: ${{ env.PASSED }}",
                "emoji": true
            }
        },
        {
            "type": "section",
            "text": {
                "type": "plain_text",
                "text": ":red_circle: Tests Failed ${{ env.FAILED }}",
                "emoji": true
            }
        },
        {
            "type": "actions",
            "elements": [
                {
                    "type": "button",
                    "text": {
                        "type": "plain_text",
                        "text": "View Results",
                        "emoji": true
                    },
                    "url": "${{ env.URL }}"
                }
            ]
        }
    ]
}

